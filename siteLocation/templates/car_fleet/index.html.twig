{% extends 'base.html.twig' %}

{% block title %}CarFleet index{% endblock %}

{% block body %}
    <h1>CarFleet index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for car_fleet in car_fleets %}
            <tr>
                <td>{{ car_fleet.id }}</td>
                <td>{{ car_fleet.status }}</td>
                <td>
                    <a href="{{ path('car_fleet_show', {'id': car_fleet.id}) }}">show</a>
                    <a href="{{ path('car_fleet_edit', {'id': car_fleet.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('car_fleet_new') }}">Create new</a>
{% endblock %}
